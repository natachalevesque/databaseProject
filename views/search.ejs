<!DOCTYPE html>
<html lang="en" dir="ltr">

<%- include('head') %>

<body>
<div class="p-5"
     style="background:transparent url('https://upload.wikimedia.org/wikipedia/commons/f/f6/Antennae_galaxies_xl.jpg')
	      no-repeat center center /cover">

        <%- include('header') %>

        <div class="p-5 mt-3 bg-transparent text-light text-center"
             style="min-height:100vh">
                <div class="jumbotron bg-light" style="opacity: .7">
                        <div class="row">
                        <div class="col">
                                <table class="table">
                                        <thead class="thead-dark">
                                        <tr>
                                                <th scope="col">Results</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <%
                                        for (var result of results ) {
                                        %>
                                        <tr>
                                                <td><a href="/result?name=<%= result.name %>&type=<%= table %>">
                                                                <%= result.name %> </a></td>
                                        </tr>
                                        <% } %>
                                        </tbody>
                                </table>
                                <hr class="my-4 row">
                        </div>
                </div>
        </div>
</div>
</body>
</html>